{
	"name": "nocode-ui",
	"version": "1.0.0",
	"description": "UI Engine for NoCode SaaS Platform",
	"source": "src/index.html",
	"browserslist": "> 0.5%, last 2 versions, not dead",
	"scripts": {
		"start": "parcel --no-cache",
		"build": "parcel build src/index.html",
		"aws": "parcel build src/index.tsx",
		"test": "jest --watchAll",
		"coverage": "jest --coverage",
		"pretty": "prettier --write ./src || prettier --write ./__tests__"
	},
	"keywords": [
		"nocode",
		"lowcode"
	],
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{ts,tsx}": [
			"prettier --write",
			"eslint --fix",
			"git add"
		],
		"*.{html,css,less,ejs}": [
			"prettier --write",
			"git add"
		]
	},
	"author": "Kiran Kumar Grandhi",
	"contributors": [
		"Kiran Kumar Grandhi",
		"Raja Avinash Alli",
		"Akhilesh Alli",
		"Surendhar Satyavarapu",
		"Tarun Jangra",
		"Md Shagil Nizami",
		"Himanshu",
		"Roopa Soumya Kandula",
		"Chavala Venktarao",
		"Sachin B",
		"Ronak Piraka"
	],
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.25.2",
		"@babel/preset-typescript": "^7.24.7",
		"@types/d3": "^7.4.3",
		"@types/jest": "^29.5.12",
		"@types/react": "^18.3.4",
		"@types/react-dom": "^18.3.0",
		"@typescript-eslint/eslint-plugin": "^8.3.0",
		"@typescript-eslint/parser": "^8.3.0",
		"buffer": "^6.0.3",
		"eslint": "^9.9.1",
		"eslint-plugin-react": "^7.35.0",
		"husky": "^9.1.5",
		"jest": "^29.7.0",
		"lint-staged": "^15.2.9",
		"parcel": "^2.12.0",
		"prettier": "^3.3.3",
		"process": "^0.11.10",
		"ts-jest": "^29.2.5"
	},
	"dependencies": {
		"@fincity/kirun-js": "^2.8.5",
		"@fincity/path-reactive-state-management": "^1.7.3",
		"@monaco-editor/react": "^4.6.0",
		"@tsconfig/recommended": "^1.0.7",
		"axios": "^1.7.5",
		"monaco-editor": "^0.51.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-router-dom": "^6.26.1"
	},
	"jest": {
		"transform": {
			".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
		},
		"testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js"
		],
		"globals": {
			"window": {}
		}
	},
	"optionalDependencies": {
		"@parcel/watcher-linux-x64-glibc": "^2.4.1"
	}
}
