{
	"name": "getAppDefinition",
	"namespace": "UIApp",
	"steps": {
		"verifyToken": {
			"statementName": "verifyToken",
			"namespace": "UIEngine",
			"name": "FetchData",
			"parameterMap": {
				"url": {
					"verifyTokenUrl": {
						"key": "verifyTokenUrl",
						"type": "VALUE",
						"value": "api/security/verifyToken"
					}
				}
			}
		},
		"ifVerifyToken": {
			"statementName": "ifVerifyToken",
			"namespace": "System",
			"name": "If",
			"parameterMap": {
				"condition": {
					"one": {
						"key": "one",
						"type": "EXPRESSION",
						"expression": "Steps.verifyToken.output.data != null"
					}
				}
			}
		},
		"setLoginData": {
			"statementName": "setLoginData",
			"namespace": "UIEngine",
			"name": "SetStore",
			"parameterMap": {
				"path": {
					"setLoginDataPath": {
						"key": "setLoginDataPath",
						"type": "VALUE",
						"value": "Store.auth"
					}
				},
				"value": {
					"setLoginDataValue": {
						"key": "setLoginDataValue",
						"type": "EXPRESSION",
						"expression": "Steps.verifyToken.output.data"
					}
				}
			},
			"dependentStatements": {
				"Steps.ifVerifyToken.true": true
			}
		},
		"applicationData": {
			"statementName": "applicationData",
			"namespace": "UIEngine",
			"name": "FetchData",
			"parameterMap": {
				"url": {
					"applicationDataUrl": {
						"key": "applicationDataUrl",
						"type": "VALUE",
						"value": "api/ui/application"
					}
				}
			},
			"dependentStatements": {
				"Steps.ifVerifyToken.output": true
			}
		},
		"ifApplicationData": {
			"statementName": "ifApplicationData",
			"namespace": "System",
			"name": "If",
			"parameterMap": {
				"condition": {
					"one": {
						"key": "one",
						"type": "EXPRESSION",
						"expression": "Steps.applicationData.output.data != null"
					}
				}
			}
		},
		"setAppData": {
			"statementName": "setAppData",
			"namespace": "UIEngine",
			"name": "SetStore",
			"parameterMap": {
				"path": {
					"setAppDataPath": {
						"key": "setAppDataPath",
						"type": "VALUE",
						"value": "Store.application"
					}
				},
				"value": {
					"setAppDataValue": {
						"key": "setAppDataValue",
						"type": "EXPRESSION",
						"expression": "Steps.applicationData.output.data"
					}
				}
			},
			"dependentStatements": {
				"Steps.ifApplicationData.true": true
			}
		},
		"themeData": {
			"statementName": "themeData",
			"namespace": "UIEngine",
			"name": "FetchData",
			"parameterMap": {
				"url": {
					"themeURL": {
						"key": "themeURL",
						"type": "VALUE",
						"value": "api/ui/theme"
					}
				}
			},
			"dependentStatements": {
				"Steps.ifVerifyToken.output": true
			}
		},
		"ifThemeData": {
			"statementName": "ifThemeData",
			"namespace": "System",
			"name": "If",
			"parameterMap": {
				"condition": {
					"one": {
						"key": "one",
						"type": "EXPRESSION",
						"expression": "Steps.themeData.output.data != null"
					}
				}
			}
		},
		"setThemeData": {
			"statementName": "setThemeData",
			"namespace": "UIEngine",
			"name": "SetStore",
			"parameterMap": {
				"path": {
					"setThemeDataPath": {
						"key": "setThemeDataPath",
						"type": "VALUE",
						"value": "Store.theme"
					}
				},
				"value": {
					"setThemeDataValue": {
						"key": "setThemeDataValue",
						"type": "EXPRESSION",
						"expression": "Steps.themeData.output.data"
					}
				}
			},
			"dependentStatements": {
				"Steps.ifThemeData.true": true
			}
		},
		"setLanguage": {
			"statementName": "setLanguage",
			"namespace": "UIEngine",
			"name": "SetStore",
			"parameterMap": {
				"path": {
					"setLanguagePath": {
						"key": "setLanguagePath",
						"type": "VALUE",
						"value": "LocalStore.currentLanguage"
					}
				},
				"value": {
					"setLanguageValue": {
						"key": "setLanguageValue",
						"type": "EXPRESSION",
						"expression": "Store.auth.localeCode??Store.application.defaultLanguage"
					}
				}
			}
		},
		"setApplicationFailureData": {
			"statementName": "setApplicationFailureData",
			"namespace": "UIEngine",
			"name": "SetStore",
			"parameterMap": {
				"path": {
					"setApplicationFailureDataPath": {
						"key": "setApplicationFailureDataPath",
						"type": "VALUE",
						"value": "Store.isApplicationLoadFailed"
					}
				},
				"value": {
					"setApplicationFailureDataValue": {
						"key": "setApplicationFailureDataValue",
						"type": "EXPRESSION",
						"expression": "null = Steps.applicationData.output.data.name"
					}
				}
			},
			"dependentStatements": {
				"Steps.applicationData.output": true
			}
		}
	}
}
